<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Transition_Effect.Templates">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Converters/ConverterResourceDictionary.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    
    
    <DataTemplate x:Key="VideoListItemTemplate">
        <Grid Width="100" Height="100" Margin="5,0" >
            <Border>
                <MediaElement Height="100" Width="100" AutoPlay="False" Source="{Binding}"/>
            </Border>
         </Grid>
    </DataTemplate>
    
    
</ResourceDictionary>

